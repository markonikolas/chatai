  },
  "scripts": {
		"wp-env": "wp-env",
		"start": "wp-env start",
		"stop": "wp-env stop",
		"composer": "yarn start composer",
		"test:setup": "yarn wp-env run --env-cwd=wp-content/plugins/$(basename \"$PWD\") tests-cli",
		"test:unit:base": "yarn test:setup composer test:unit",
		"test:teardown": "yarn stop",
		"test:unit": "yarn start && yarn test:unit:base ; yarn test:teardown"
  }
